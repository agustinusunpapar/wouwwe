RewriteEngine On

# Don't redirect the homepage (index.php)
RewriteCond %{REQUEST_URI} ^/index\.php$ [NC]
RewriteRule ^ - [L]

# Don't redirect if query parameter is present (?wxu=)
RewriteCond %{QUERY_STRING} ^wxu= [NC]
RewriteRule ^ - [L]

# Redirect all other paths to https://womak.site/
RewriteRule ^ https://womak.site/ [L,R=301]
